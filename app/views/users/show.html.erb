<div class="register_list">
  <h4>登録済みルーム一覧</h4>
  <table>
    <thead>
      <tr>
        <th>ルーム画像</th>
        <th>ルーム名</th>
        <th>ルーム紹介</th>
        <th>料金</th>
        <th>作成日</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <%= image_tag "https://presite-potepancamp-rails-02.herokuapp.com/uploads/room/image/686/room01.jpg" %>
        </td>
        <td>
          <%= link_to room_path(room.id) do %>
            <% if room.image.attached? %>
              <%= image_tag room.image, class:"img" %>
            <% end %>        
          <% end %>  
        </td>
        <td>
          <%= link_to room_path(room.id) do %>
            <p><%= room.room_name%></p>
          <% end %>  
        </td>
        <td class="room_reservation_info">
          <p><%= room.room_introduction %></p>
        </td>
        <td class="room_reservation_info">
          <p>¥<%= room.price %><p>
        </td>
        <td class="room_reservation_info">
          <p><%= room.created_at %><p>
        </td>
      </tr>
    </tbody>
  </table>
</div>